{
  "name": "8leeai",
  "version": "1.0.1",
  "private": true,
  "packageManager": "bun@1.3.9",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "lsof -ti:1333 | xargs kill -9 2>/dev/null || true && rm -rf .next .turbo node_modules/.cache && next dev -p 1333 --turbo",
    "dev:clean": "rm -rf .next node_modules/.cache && bun run dev",
    "build": "rm -rf .next && next build",
    "start": "next start -p 1333",
    "lint": "bunx biome check .",
    "format": "bunx biome format --write .",
    "check": "bunx biome check --write .",
    "test": "bun run test:smoke",
    "test:smoke": "bun test tests/smoke",
    "check:full": "bun run lint && bun run test:smoke && bun run build && bunx knip",
    "clean": "rm -rf .next node_modules/.cache .turbo"
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@tailwindcss/postcss": "^4.2.1",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.24",
    "knip": "^5.85.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
