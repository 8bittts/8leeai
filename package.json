{
  "name": "8leeai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.0",
  "scripts": {
    "dev": "lsof -ti:1333 | xargs kill -9 2>/dev/null || true && rm -rf .next .turbo node_modules/.cache && next dev -p 1333 --turbo",
    "dev:clean": "rm -rf .next node_modules/.cache && bun run dev",
    "build": "rm -rf .next && next build",
    "start": "next start -p 1333",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "bunx biome check .",
    "format": "bunx biome format --write .",
    "check": "bunx biome check --write .",
    "clean": "rm -rf .next node_modules/.cache .turbo",
    "packages": "bun scripts/package-monitor.js",
    "packages:watch": "bun scripts/package-monitor.js --watch",
    "packages:critical": "bun scripts/package-monitor.js --critical"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "next": "15.5.6",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "happy-dom": "20.0.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  }
}
